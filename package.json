{
  "name": "node-kafka-demo",
  "version": "1.0.0",
  "description": "Node express typescript template",
  "scripts": {
    "document": "typedoc --out docs src",
    "test": "jest --ci  --reporters=default --detectOpenHandles",
    "test:debug": "node --inspect-brk ../node_modules/.bin/jest --runInBand",
    "coverage": "jest --ci --collectCoverage",
    "build": "tsc",
    "copyStaticFiles": "cp src/api/open-api.yaml dist/src/api/open-api.yaml",
    "build:prod": "npm-run-all build copyStaticFiles",
    "start:dev": "tsnd ./src/server.ts",
    "start:debug": "tsnd --inspect=9229 src/server.ts",
    "start": "NODE_ENV=production node dist/src/server.js",
    "prettify": "prettier --write 'src/**/*.ts'",
    "lint": "tslint ./src/**/*.ts --fix"
  },
  "author": "Alexander Davies",
  "license": "MIT",
  "engines": {
    "node": ">=12.16.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-openapi-validate": "^0.5.1",
    "express-rate-limit": "^5.1.3",
    "express-slow-down": "^1.3.1",
    "express-validator": "^6.6.1",
    "ip": "^1.1.5",
    "kafkajs": "^1.14.0",
    "lodash": "^4.17.20",
    "prettier": "^2.1.1",
    "swagger-routes-express": "^3.2.1",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-slow-down": "^1.3.0",
    "@types/ip": "^1.1.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@types/yamljs": "^0.2.31",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "26.5.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.4.1",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  }
}
